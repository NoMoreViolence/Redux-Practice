{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "no-console": [
      true,
      "error",
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "interface-name": false,
    "ordered-imports": false,
    "interface-over-type-literal": false
  },
  "no-shadowed-variable": false,
  "array-type": false,
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
